{
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/node-api/(.*)$",
      "destination": "node-backend",
      "target": "/$1",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/user-api/(.*)$",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^(.*)$",
      "localDir": "../gmui/webapp"
    }
    
  ],
  "welcomeFile": "/indexdev.html"
}